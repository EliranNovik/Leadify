# 1com API Configuration
# Get these values from your 1com PBX Configuration/Settings page

# 1com API Key (Read/Write access recommended for full functionality)
ONECOM_API_KEY=your_api_key_here

# 1com Tenant Code (e.g., "DEMO", "decker", etc.)
ONECOM_TENANT=your_tenant_code_here

# 1com Base URL (usually this is the default)
ONECOM_BASE_URL=https://pbx6webserver.1com.co.il/pbx/proxyapi.php

# Example values (replace with your actual values):
# ONECOM_API_KEY=FwLjNxpSyN8tNKmD
# ONECOM_TENANT=DEMO
# ONECOM_BASE_URL=https://pbx6webserver.1com.co.il/pbx/proxyapi.php
